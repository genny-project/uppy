{
  "name": "uppy-server",
  "version": "0.10.0",
  "description": "Server component for Uppy's (https://uppy.io) extensible file upload widget with support for drag&drop, resumable uploads, previews, restrictions, file processing/encoding, remote providers like Dropbox and Google Drive, S3 and more :dog:",
  "main": "lib/uppy.js",
  "types": "types/index.d.ts",
  "author": "Transloadit.com",
  "license": "ISC",
  "homepage": "https://github.com/transloadit/uppy-server#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transloadit/uppy-server.git"
  },
  "keywords": [
    "file uploader",
    "progress",
    "preview",
    "resumable uploads",
    "tus",
    "s3",
    "google drive",
    "dropbox",
    "backend",
    "websocket",
    "express",
    "realtime"
  ],
  "bin": {
    "uppy-server": "./lib/standalone/start-server.js"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "pre-commit": "lint-staged",
  "devDependencies": {
    "@types/connect-redis": "0.0.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/express-session": "^1.15.6",
    "@types/helmet": "0.0.37",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/lodash.merge": "^4.6.3",
    "@types/morgan": "^1.7.35",
    "@types/ms": "^0.7.30",
    "@types/node": "^8.5.1",
    "@types/request": "^2.0.9",
    "@types/tus-js-client": "^1.4.1",
    "@types/uuid": "^3.4.3",
    "@types/ws": "^3.2.1",
    "babel-cli": "6.18.0",
    "babel-jest": "19.0.0",
    "babel-preset-es2015": "6.18.0",
    "eslint": "2.4.0",
    "eslint-config-standard": "5.1.0",
    "eslint-plugin-promise": "1.0.8",
    "eslint-plugin-standard": "1.3.2",
    "fakefile": "0.0.8",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "lint-staged": "3.2.1",
    "nodemon": "1.8.1",
    "pre-commit": "1.1.3",
    "supertest": "3.0.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "@purest/providers": "1.0.0",
    "atob": "2.0.3",
    "aws-sdk": "^2.77.0",
    "body-parser": "1.18.2",
    "connect-redis": "^3.3.0",
    "cookie-parser": "1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.16.0",
    "express-prom-bundle": "^3.1.0",
    "express-session": "1.15.6",
    "grant-express": "3.8.0",
    "helmet": "3.8.2",
    "jsonwebtoken": "^8.0.1",
    "lodash.merge": "^4.6.0",
    "morgan": "1.9.0",
    "ms": "^2.0.0",
    "prom-client": "^10.0.2",
    "purest": "3.0.0",
    "randomstring": "^1.1.5",
    "redis": "^2.7.1",
    "request": "^2.81.0",
    "tus-js-client": "1.4.3",
    "uuid": "2.0.2",
    "validator": "^9.0.0",
    "ws": "1.1.5"
  },
  "files": [
    "lib/"
  ],
  "jest": {
    "testEnvironment": "node",
    "automock": false,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**"
    ]
  },
  "scripts": {
    "build": "tsc -p .",
    "console": "source env.sh && ${FREYCMD:-frey} remote --project-dir ./infra",
    "deploy:travis": "bash -c 'source env.sh && env DEBUG=\"\" frey install --force-yes --project-dir ./infra'",
    "deploy:localfrey": "env FREYCMD=\"babel-node ${HOME}/code/frey/src/cli.js\" npm run deploy",
    "deploy": "source env.sh && ${FREYCMD:-frey} setup --project-dir ./infra",
    "fix": "eslint . --fix",
    "install:frey": "source env.sh && ${FREYCMD:-frey} refresh --bail --project-dir ./infra",
    "launch": "source env.infra.sh && ${FREYCMD:-frey} infra --project-dir ./infra",
    "lint": "eslint .",
    "lint-staged": "lint-staged",
    "logtail:localfrey": "env FREYCMD=\"babel-node ${HOME}/code/frey/src/cli.js\" npm run logtail",
    "logtail": "source env.sh && ${FREYCMD:-frey} remote --project-dir ./infra --remote 'tail -f /srv/uppy-server/shared/logs/uppy-server.log'",
    "prepublish": "npm run build",
    "release:major": "env SEMANTIC=major npm run release",
    "release:minor": "env SEMANTIC=minor npm run release",
    "release:patch": "env SEMANTIC=patch npm run release",
    "release": "npm version ${SEMANTIC:-patch} -m \"Release %s\" && git push && git push --tags && npm publish",
    "start:production": "node ./lib/standalone/start-server.js",
    "start": "bash -c 'source env.sh; env UPPYSERVER_DATADIR=./output UPPYSERVER_DOMAIN=localhost:3020 UPPYSERVER_PROTOCOL=http UPPYSERVER_PORT=3020 UPPY_ENDPOINT=localhost:3000 UPPY_ENDPOINTS=localhost:3000 nodemon ./src/standalone/start-server.js --exec node'",
    "start:40": "bash -c 'source env.sh; env UPPYSERVER_DATADIR=./output UPPYSERVER_DOMAIN=localhost:3040 UPPYSERVER_PROTOCOL=http UPPYSERVER_PORT=3040 UPPY_ENDPOINT=localhost:3452 UPPY_ENDPOINTS=localhost:3452 nodemon ./src/standalone/start-server.js --exec node'",
    "test": "/bin/bash -c 'npm run build && npm run lint && source env.test.sh && jest'",
    "test:watch": "npm test -- --watch"
  }
}
